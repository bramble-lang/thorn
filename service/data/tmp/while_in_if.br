fn my_main() -> i64 {
    test(0);
    test(1);
    return 0;
}

fn test(n: i64) {
    if (n == 0) {
        let mut x: i64 := 0;
        while (x <= 5) {
            project::std::io::writei64ln(x);
            mut x := x + 1;
        };
    } else {
        let mut x: i64 := 5;
        while (x >= 0) {
            project::std::io::writei64ln(x);
            mut x := x - 1;
        };
    };

    return;
}